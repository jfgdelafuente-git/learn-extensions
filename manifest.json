{
    "manifest_version": 3,
    "name": "MyExtension",
    "version": "1.0",
    "description": "Changes the background color of the current tab",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.jpeg"
      }
    }
  }
  